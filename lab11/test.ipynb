   "source": [
    "def repeat(f,n):\n",
    "    def helper(x):\n",
    "        if n!=0:\n",
    "            return f(x)*repeat(f,n-1)(x)\n",
    "        else:\n",
    "            return 1\n",
    "    return helper\n"
   ]
